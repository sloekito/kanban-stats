apiVersion: v1
kind: Service
metadata:
  name: <%= name %>
  labels:
    app: <%= app_name %>
    stack: <%= stack %>
spec:
  type: NodePort
  selector:
    app: <%= app_name %>
    deploy: <%= deploy_hex %>
  ports:
    # The port that this service should serve on.
  - name: http
    port: <%= service_port %>
    targetPort: <%= container_port %>
